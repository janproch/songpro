{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "SongPro",
	"patterns": [
		{
			"include": "#chords"
		}
	],
	"repository": {
		"chords": {
			"patterns": [{
				"name": "entity.name.tag.chord.songpro",
				"match": "\\[([^\\]]*)\\]",
				"captures": {
					"0": {
						"name": "entity.name.tag.chord.songpro"
					},
					"1": {
						"name": "string.other.chord-name.songpro"
					}
				}
			}]
		}
	},
	"scopeName": ".source.songpro"
}